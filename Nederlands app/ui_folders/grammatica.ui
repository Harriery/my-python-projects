QPushButton:hover {
    background-color: #76C7C0; /* Daha açık yeşil */
    border: 2px solid #5EAAA8;
}

/* Tıklayınca */
QPushButton:pressed {
    background-color: #EAF4F4; /* Açık renk */
    border: 2px solid #5EAAA8;
}

/* Butonun şeffaf olmasını önlemek için */
QPushButton:!disabled {
    background: #5EAAA8; 
}
